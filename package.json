{
  "name": "mirailens",
  "version": "1.0.0",
  "description": "MCP server for browser automation using MiraiLens - enables AI assistants to control web browsers",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "browser-automation",
    "web-automation",
    "accessibility",
    "ai-assistant",
    "claude",
    "mirailens"
  ],
  "author": {
    "name": "MiraiLens",
    "url": "https://mirailens.io"
  },
  "license": "MIT",
  "homepage": "https://github.com/mugenkyou/mcp-server-mirailens#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mugenkyou/mcp-server-mirailens.git"
  },
  "bugs": {
    "url": "https://github.com/mugenkyou/mcp-server-mirailens/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mirailens": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup src/index.ts --format esm && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "watch": "tsup src/index.ts --format esm --watch",
    "clean": "rm -rf dist/",
    "inspector": "CLIENT_PORT=9001 SERVER_PORT=9002 pnpx @modelcontextprotocol/inspector node dist/index.js",
    "dev": "npm run watch",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "commander": "^13.1.0",
    "mirailens": "^1.0.0",
    "ws": "^8.18.1",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.3"
  },
  "devDependencies": {
    "@types/ws": "^8.18.0",
    "shx": "^0.3.4",
    "tsup": "^8.4.0",
    "typescript": "^5.6.2"
  }
}
